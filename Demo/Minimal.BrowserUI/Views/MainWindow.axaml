<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:Minimal.Demo.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:Minimal.Demo.Views"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Minimal.BrowserUI.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="Minimal.Demo">
	<DockPanel>
		<Menu Name="mainMenu" DockPanel.Dock="Top">
			<MenuItem Header="_File">
				<MenuItem x:Name="MenuDevtool" Header="_Open Dev Tools..." />
			</MenuItem>
		</Menu>
		<Panel x:Name="MainContainer"/>
	</DockPanel>	
</Window>
